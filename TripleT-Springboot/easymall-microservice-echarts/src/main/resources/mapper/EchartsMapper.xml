<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.scnu.echarts.mapper.EchartsDao">
<select id="showOrderByTime" resultType="com.easymall.common.pojo.Order">
	SELECT * FROM t_order ORDER BY order_time
</select>
<select id="showProductBySaleNum" resultType="easymall.po.Products">
	SELECT * FROM products ORDER BY soldnum
</select>
<select id="showCategory" resultType="easymall.po.Category">
	SELECT * FROM category
</select>
</mapper>